{
    "name": "perf2k2/monitre",
    "type": "library",
    "description": "PHP library for remote linux monitoring/diagnostic with no installation needed",
    "keywords": ["linux monitoring", "os monitoring", "remote monitoring", "remote diagnostic"],
    "license": "MIT",
    "authors": [
        {
            "name": "Perf2k2",
            "email": "perf2k2@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.2.0"
        }
    },
    "autoload": {
        "psr-4": {
            "Perf2k2\\Monitre\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "vlucas/phpdotenv": "2.*"
    },
  "require": {
    "php": ">=7.2",
    "ext-ssh2": "*"
  }
}